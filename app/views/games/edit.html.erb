<main id="main" role="main">
    <div class="container-card">
        <%= bootstrap_form_for(@game) do |f| %>
        <%= f.text_field :game_name %>
        <%= f.text_field :description %>
        <%= f.text_field :price %>
        <%= f.file_field :picture %>
        <%= f.submit "Submit" %>
        <% end %>
    </div>
</main>